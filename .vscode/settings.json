{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "cSpell.language": "en,es-ES",
  "cSpell.words": [
    "ctas",
    "geist",
    "logomark",
    "monolab",
    "rgba",
    "tailwindcss",
    "turbopack"
  ]
}
